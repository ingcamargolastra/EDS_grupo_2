   "source": [
    "def fibonacci_recursivo(n):\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    return fibonacci_recursivo(n - 1) + fibonacci_recursivo(n - 2)\n",
    "\n",
    "n = input(\"Ingrese el N-esimo termino a ver de la serie: \")\n",
    "\n",
    "print(fibonacci_recursivo(int(n)))"
   ]
