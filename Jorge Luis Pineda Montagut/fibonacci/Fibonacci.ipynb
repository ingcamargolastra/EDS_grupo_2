    "def fibonacci(n):\n",
    "    idx = 0\n",
    "    a, b = 1, 1\n",
    "    while idx < n:\n",
    "        if(idx == n - 1):\n",
    "            return a\n",
    "        idx = idx + 1\n",
    "        a, b = b, a + b\n",
    "n = input(\"Ingrese el N-esimo término a ver en la serie: \")\n",
    "fibonacci(int(n))"
