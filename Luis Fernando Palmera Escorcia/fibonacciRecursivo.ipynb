   "source": [
    "n = int(input(\"Ingrese valor \" ))\n",
    "def calcularFibonacci (n):\n",
    "\n",
    "    if (n == 0):\n",
    "        return 0\n",
    "    if(n == 1):\n",
    "        return 1\n",
    "    return calcularFibonacci(n-1) + calcularFibonacci(n-2)\n",
    "    \n",
    "print(\"El valor de la serie es: \",calcularFibonacci(n))"
   ]
